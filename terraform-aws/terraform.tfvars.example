# Copy this file to terraform.tfvars and fill in your values
# DO NOT commit terraform.tfvars to git!

aws_region  = "ap-southeast-1"
iam_name    = "cheeyoung"
vpc_cidr    = "10.1.0.0/16"
key_name    = "keypair-cheeyoung"

# Your public IP for SSH access (find it at https://whatismyipaddress.com)
# Format: "x.x.x.x/32"
my_ip = "YOUR_PUBLIC_IP/32"

# RDS database password (choose a strong password)
db_password = "YourStrongPassword123!"
db_username = "root"

# Instance types (use t2.micro for free tier)
instance_type    = "t2.micro"
db_instance_class = "db.t3.micro"

# After creating AMI from webserver, add this line:
# webserver_ami_id = "ami-xxxxxxxxxxxxxxxxx"
